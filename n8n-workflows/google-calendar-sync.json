{
  "name": "Google Calendar Sync",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "minutes",
              "minutesInterval": 5
            }
          ]
        }
      },
      "id": "schedule-trigger",
      "name": "Every 5 Minutes",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [260, 300]
    },
    {
      "parameters": {
        "httpMethod": "GET",
        "path": "calendar-sync",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "manual-webhook",
      "name": "Manual Sync Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [260, 500],
      "webhookId": "calendar-sync"
    },
    {
      "parameters": {
        "method": "GET",
        "url": "=https://dental-dashboard-nu.vercel.app/api/n8n/availability?date={{ $now.plus({days: 1}).format('yyyy-MM-dd') }}",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "X-API-Key",
              "value": "79d22dce33bd515d33124461ecb3202c27e660944ef29b2cb3cdf3396d37c6e4"
            }
          ]
        },
        "options": {}
      },
      "id": "get-doctors",
      "name": "Get All Doctors",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [480, 400]
    },
    {
      "parameters": {
        "jsCode": "// Extract doctors array from API response\nconst response = $input.first().json;\nconst doctors = response.doctors || [];\n\n// Filter doctors with calendar_id and return each as separate item\nconst doctorsWithCalendar = doctors.filter(d => d.calendar_id && d.calendar_id.trim());\n\nconsole.log(`Found ${doctorsWithCalendar.length} doctors with calendar IDs`);\n\nreturn doctorsWithCalendar.map(doctor => ({ json: doctor }));"
      },
      "id": "extract-doctors",
      "name": "Extract Doctors With Calendars",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [700, 400]
    },
    {
      "parameters": {
        "batchSize": 1,
        "options": {}
      },
      "id": "split-batches",
      "name": "Process One Doctor",
      "type": "n8n-nodes-base.splitInBatches",
      "typeVersion": 3,
      "position": [920, 400]
    },
    {
      "parameters": {
        "jsCode": "// Prepare Google Calendar query for this doctor\nconst doctor = $input.first().json;\n\n// Get events from last 24 hours to next 30 days\nconst timeMin = new Date();\ntimeMin.setHours(timeMin.getHours() - 24);\n\nconst timeMax = new Date();\ntimeMax.setDate(timeMax.getDate() + 30);\n\nconsole.log(`Fetching calendar for doctor: ${doctor.name}, calendar: ${doctor.calendar_id}`);\n\nreturn [{\n  json: {\n    doctorId: doctor.id,\n    doctorName: doctor.name,\n    clinicId: doctor.clinic_id,\n    calendarId: doctor.calendar_id,\n    timeMin: timeMin.toISOString(),\n    timeMax: timeMax.toISOString()\n  }\n}];"
      },
      "id": "prepare-query",
      "name": "Prepare Calendar Query",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1140, 400]
    },
    {
      "parameters": {
        "operation": "getAll",
        "calendar": {
          "__rl": true,
          "value": "={{ $json.calendarId }}",
          "mode": "id"
        },
        "returnAll": false,
        "limit": 100,
        "options": {
          "timeMax": "={{ $json.timeMax }}",
          "timeMin": "={{ $json.timeMin }}"
        }
      },
      "id": "google-calendar",
      "name": "Get Calendar Events",
      "type": "n8n-nodes-base.googleCalendar",
      "typeVersion": 1.2,
      "position": [1360, 400],
      "credentials": {
        "googleCalendarOAuth2Api": {
          "id": "google-calendar-creds",
          "name": "Google Calendar"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Transform Google Calendar events to appointments format\n// IMPORTANT: Get doctor context from the Prepare Calendar Query node for THIS batch\nconst events = $input.all();\nconst queryData = $('Prepare Calendar Query').first().json;\n\nconsole.log(`Processing ${events.length} events for doctor ${queryData.doctorName}`);\n\nif (events.length === 0) {\n  return [];\n}\n\nreturn events.map(event => {\n  const e = event.json;\n  \n  // Parse patient info from event title/description\n  let patientName = 'Неизвестен';\n  let patientPhone = null;\n  let appointmentType = 'Преглед';\n  \n  // Try to extract name from title (format: \"Name - Type\" or just \"Name\")\n  if (e.summary) {\n    const parts = e.summary.split(' - ');\n    patientName = parts[0].trim();\n    if (parts[1]) appointmentType = parts[1].trim();\n  }\n  \n  // Try to extract phone from description\n  if (e.description) {\n    const phoneMatch = e.description.match(/\\d{9,12}/);\n    if (phoneMatch) patientPhone = phoneMatch[0];\n  }\n  \n  return {\n    json: {\n      googleEventId: e.id,\n      clinicId: queryData.clinicId,\n      doctorId: queryData.doctorId,\n      doctorName: queryData.doctorName,\n      patientName,\n      patientPhone,\n      appointmentType,\n      startTime: e.start?.dateTime || e.start?.date,\n      endTime: e.end?.dateTime || e.end?.date,\n      status: e.status === 'cancelled' ? 'cancelled' : 'scheduled',\n      source: 'google_calendar'\n    }\n  };\n});"
      },
      "id": "transform-events",
      "name": "Transform to Appointments",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1580, 400]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "has-events",
              "leftValue": "={{ $json.googleEventId }}",
              "rightValue": "",
              "operator": {
                "type": "string",
                "operation": "isNotEmpty"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "filter-events",
      "name": "Has Events",
      "type": "n8n-nodes-base.filter",
      "typeVersion": 2,
      "position": [1800, 400]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://dental-dashboard-nu.vercel.app/api/n8n/sync-calendar",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "X-API-Key",
              "value": "79d22dce33bd515d33124461ecb3202c27e660944ef29b2cb3cdf3396d37c6e4"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ JSON.stringify({ appointments: $input.all().map(i => i.json) }) }}",
        "options": {}
      },
      "id": "sync-appointments",
      "name": "Sync to Dashboard",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [2020, 400]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ JSON.stringify({ success: true, message: 'Calendar sync triggered' }) }}",
        "options": {}
      },
      "id": "webhook-response",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1.1,
      "position": [2240, 400]
    },
    {
      "parameters": {
        "jsCode": "// Log sync result\nconst result = $input.first().json;\nconsole.log('Sync completed:', result);\nreturn [{ json: result }];"
      },
      "id": "log-result",
      "name": "Log Result",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [2240, 600]
    }
  ],
  "connections": {
    "Every 5 Minutes": {
      "main": [
        [
          {
            "node": "Get All Doctors",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Manual Sync Webhook": {
      "main": [
        [
          {
            "node": "Get All Doctors",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Get All Doctors": {
      "main": [
        [
          {
            "node": "Extract Doctors With Calendars",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Extract Doctors With Calendars": {
      "main": [
        [
          {
